@model Vulcan.Models.VulcanTransaction

@{
    ViewData["Title"] = "Create";
}

<h1>Create VulcanTransaction</h1>
<hr />

@using (Html.BeginForm("Create", "VulcanTransactions", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <input type="file" value="Upload CSV" name="CsvFile" class="btn btn-primary" required />
                    <input type="submit" value="CreateForm" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>
}

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
